{
  "name": "pnp-frontend",
  "version": "1.0.0",
  "main": "index.html",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "export NODE_OPTIONS=--max_old_space_size=8096 && vite build",
    "start": "vite preview --port 1234",
    "dev:production": "export VITE_SANITY_DATASET=production && vite",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:staged": "lint-staged",
    "format": "prettier . --write",
    "deploy": "yarn install --production=false && yarn build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@azure/msal-browser": "^2.22.0",
    "@azure/msal-react": "^1.4.5",
    "@radix-ui/react-icons": "^1.1.1",
    "@react-pdf/renderer": "^3.0.0",
    "@sanity/block-content-to-markdown": "^0.0.5",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/block-tools": "^2.27.0",
    "@sanity/client": "^3.1.0",
    "@sanity/schema": "^2.27.0",
    "@types/events": "^3.0.0",
    "@types/ramda": "^0.28.15",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "events": "^3.3.0",
    "focus-trap-react": "^10.0.0",
    "marked": "^4.0.12",
    "pnp-shared": "*",
    "polished": "^4.1.4",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-helmet": "^6.1.0",
    "react-pdf": "^5.7.2",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.4.0",
    "react-use": "^17.4.0",
    "react-uuid": "^1.0.2",
    "styled-components": "^5.1.1",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@sanity/types": "^3.9.0",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.5",
    "@types/react-pdf": "^5.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.24",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.1",
    "typescript": "^4.6.2",
    "vite": "^3.2.2"
  }
}
